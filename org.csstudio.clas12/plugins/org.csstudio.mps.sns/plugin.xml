<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.csstudio"
            name="CSS">
      </category>
      <view
            category="org.csstudio"
            class="org.csstudio.mps.sns.view.MPSBrowserView"
            icon="icons/mps.JPG"
            id="org.csstudio.mps.sns.view.MPSBrowserView"
            name="MPS Browser"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Global CSS menu actions"
            id="org.csstudio.mps.sns"
            label="CSS Action Set"
            visible="true">
         <action
               class="org.csstudio.mps.sns.view.NewMPSBrowserAction"
               icon="icons/mps.JPG"
               id="org.csstudio.mps.sns.view.NewMPSAction"
               label="MPS Browser"
               menubarPath="css/utility/"
               style="push">
         </action>
      </actionSet>
   </extension>
</plugin>
